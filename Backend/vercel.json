{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./ss-api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/ss",
      "dest": "/ss-api"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
